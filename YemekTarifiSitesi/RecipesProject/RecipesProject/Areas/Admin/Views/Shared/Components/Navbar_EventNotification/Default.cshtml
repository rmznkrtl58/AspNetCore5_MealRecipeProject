@model List<EventListDTO>
<div class="dropdown-menu dropdown-menu-end navbar-dropdown preview-list" aria-labelledby="notificationDropdown">
    <h6 class="p-3 mb-0">Duyurular</h6>
    @foreach(var x in Model){
    <div class="dropdown-divider"></div>
    <a href="/Admin/Event/EditEvent/@x.EventId" class="dropdown-item preview-item">
        <div class="preview-thumbnail">
            <div class="preview-icon bg-dark rounded-circle">
                <i class="mdi mdi-calendar text-success"></i>
            </div>
        </div>
        <div class="preview-item-content">
                <p class="preview-subject mb-1">@x.MainHeader</p>
        </div>
    </a>
    @* <div class="dropdown-divider"></div> *@
    }
    <p class="p-3 mb-0 text-center">@Model.Count Duyuru Mevcud</p>
</div>
