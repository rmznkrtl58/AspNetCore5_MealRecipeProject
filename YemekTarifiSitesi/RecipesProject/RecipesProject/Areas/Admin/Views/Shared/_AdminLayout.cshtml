@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("/Areas/Admin/Views/AdminLayout/HeadPartial.cshtml")
<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_sidebar.html -->
        @await Html.PartialAsync("/Areas/Admin/Views/AdminLayout/SidebarPartial.cshtml")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_navbar.html -->
            @await Html.PartialAsync("/Areas/Admin/Views/AdminLayout/NavbarPartial.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="page-header">
                        <h3 class="page-title"> @ViewBag.k1 </h3>
                    </div>
                    <div class="row">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("/Areas/Admin/Views/AdminLayout/ScriptPartial.cshtml")
</body>
</html>
