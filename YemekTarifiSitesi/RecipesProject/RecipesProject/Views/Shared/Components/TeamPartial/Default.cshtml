@model List<TeamListDTO>
<section id="chefs" class="chefs section">

   
    <div class="container section-title" data-aos="fade-up">
        <h2>EKİP ARKADAŞLARIMIZ</h2>
        <p>Sitemizin Her Türlü Desteğini Sağlayan Ekip Arkadaşlarımızı Tanıyın.</p>
    </div>

    <div class="container">

        <div class="row gy-4">
            @foreach(var x in Model){
            <div class="col-lg-4" data-aos="fade-up" data-aos-delay="100">
                <div class="member">
                    <img src="@x.Image" class="img-fluid" style="height:430px;width:400px" alt="">
                    <div class="member-info">
                        <div class="member-info-content">
                            <h4>@x.NameSurname</h4>
                            <span>@x.Status</span>
                        </div>
                        <div class="social">
                            <a href="@x.TwitterUrl"><i class="bi bi-twitter-x"></i></a>
                            <a href="@x.InstagramUrl"><i class="bi bi-instagram"></i></a>
                            <a href="@x.LinkedinUrl"><i class="bi bi-linkedin"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            }
        </div>
    </div>
</section>